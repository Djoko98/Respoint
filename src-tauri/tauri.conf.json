{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "ResPoint",
  "version": "0.1.7",
  "identifier": "com.respoint.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "ResPoint - Restaurant Management System",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "maximized": true,
        "decorations": false,
        "transparent": false,
        "center": true
      }
    ],
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
  ,
  "plugins": {
    "updater": {
      "active": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDVFMUI5NTJERTk4NkE1Q0UKUldUT3BZYnBMWlViWGg1WWJSbW5iU1FxN29hd0FZZ0phK1BQTng5cW5IOWhSV09wbkpWM3pzUTUK",
      "endpoints": [
        "https://github.com/Djoko98/Respoint/releases/latest/download/latest.json"
      ],
      "dialog": true,
      "windows": {
        "installMode": "basicUi"
      }
    }
  }
}
